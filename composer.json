{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2",
        "ext-iconv": "*",
        "api-platform/core": "^2.1",
        "cspoo/swiftmailer-mailgun-bundle": "^1.2",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^1.6.10|^2.0",
        "doctrine/doctrine-migrations-bundle": "^1.3|^2.0",
        "doctrine/orm": "^2.5.11",
        "lexik/jwt-authentication-bundle": "^2.4",
        "nelmio/cors-bundle": "^1.5 || ^2.0",
        "payum/paypal-express-checkout-nvp": "^1.5",
        "payum/payum-bundle": "^2.3",
        "php-http/guzzle6-adapter": "^1.1",
        "phpdocumentor/reflection-docblock": "^3.0|^4.0",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/asset": "^4.0",
        "symfony/console": "^4.0",
        "symfony/expression-language": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/monolog-bundle": "^3.1",
        "symfony/process": "^4.0",
        "symfony/property-access": "^4.0",
        "symfony/property-info": "^4.0",
        "symfony/security-bundle": "^4.0",
        "symfony/serializer": "^4.0",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/twig-bundle": "^4.0",
        "symfony/validator": "^4.0",
        "symfony/web-link": "^4.0",
        "symfony/webpack-encore-bundle": "1.17.1",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "behat/behat": "^3.4",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-extension": "^2.3",
        "behat/symfony2-extension": "^2.1",
        "doctrine/data-fixtures": "^1.3",
        "easycorp/easy-log-handler": "^1.0.7",
        "guzzlehttp/guzzle": "^6.3",
        "hautelook/alice-bundle": "^2.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "^3.2",
        "symfony/debug-bundle": "^4.0",
        "symfony/stopwatch": "^4.0",
        "symfony/var-dumper": "^4.0",
        "symfony/web-profiler-bundle": "^4.0",
        "theofidry/alice-data-fixtures": "^1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "App\\Behat\\": "behat/src/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C9PQSVZ67T5F0HGFTAQB5D7X",
            "allow-contrib": false
        }
    }
}
